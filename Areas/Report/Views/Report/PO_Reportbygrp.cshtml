
@{
    ViewBag.Title = "PO_Reportbygrp";
    Layout = "~/Areas/CPanel/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="panel-body">
    
</div>


<div class="panel-body inprdservice">
    <div class="col-md-12 inprdservicehead" style="margin-bottom:0;">
        <h5>PO Report</h5>
    </div>

    <div class="row">
<section class="social" id="ReportDisplayPanel">
            <div class="myIframe col-md-12 table-responsive">
                <div class="panel-body"><iframe id="ifrmorderRpt" frameborder="0" scrolling="auto"></iframe></div>
            </div>
        </section>
    </div>
</div>


@Scripts.Render("~/bundles/jquery")

<script>
    $(document).ready(function () {
        Reportbygroup();
    });
    function Reportbygroup()
    {
        var myPoNumber = '@ViewBag.Po'
        //alert(myPoNumber);
        url = "../../Areas/Report/Podetailsbygrp.aspx?PO=" + myPoNumber;
        var myframe = document.getElementById("ifrmorderRpt");
        if (myframe !== null) {
            if (myframe.src) {
                myframe.src = url;
            }
            else if(myframe.contentWindow!==null && myframe.contentWindow.location!==null)
            {
                myframe.contentWindow.location = url;
            }
            else{myframe.setAttribute('src',url)}
        }
    }


</script>


