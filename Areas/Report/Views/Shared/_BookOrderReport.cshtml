 @model List<PrachiIndia.Sql.USP_GET_RETAIL_ORDERS_Result>
<div class="panel panel-default">
    <div class="panel-heading">
        <h4 id="idTotal" class="card-title">Order Details</h4>
    </div>
    <div class="panel-body">

        <table id="order-details" class="table table-striped table-bordered" style="width:100%">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">S.NO#</th>
                    <th scope="col">Title</th>
                    <th scope="col">Quantity</th>
                    <th scope="col">Class</th>
                    <th scope="col">Amount</th>
                    <th scope="col">Date</th>
                </tr>
            </thead>
            <tbody>
    @{int count = 1; }
    @foreach (var Item in Model)
    {
        if (Item.Class == "")
        {
            <tr>
                <td colspan="6"><h3 style="color:red; text-align:center">No Record Found</h3></td>
            </tr>
        }
        else
        {
                    <tr>
                        <td>@count</td>
                        <td>@Item.Title</td>
                        <td>@Item.Quantity</td>
                        <td>@Item.Class</td>
                        @*<td>@Item.TotalAmount</td>*@
                        <td>@Item.CreatedDate</td>
                    </tr>
                    }
                    count = count + 1;
                }

            </tbody>

        </table>

    </div>
</div>






