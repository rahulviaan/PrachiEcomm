@model IEnumerable<PrachiIndia.Portal.Models.OrderTrackDetailVm>
@{
    ViewBag.Title = "dispatchUserOrderDetail";
    //Layout = "~/Areas/CPanel/Views/Shared/_Layout.cshtml";
}
<div style="margin-left:250px;margin-top:50px;width:80%">
    <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>OrderId</th>
                <th>TransactionId</th>
                <th>Status</th>
                <th>Amount</th>
                @*<th>Date</th>*@

                @*<th>DispatchBy</th>*@
                <th>BillingAddress</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var m in Model)
            {
                <tr>
                    <td>
                        <a href="Admin/OrderDetail?OrderId=@m.orderId" target="_blank"><span>@m.orderId</span></a>
                    </td>
                    <td><span>@m.TransactionId</span></td>
                    <td><i class="fa fa-inr"></i><span>@m.Status</span></td>
                    <td><i class="fa fa-inr"></i><span>@m.Amount</span></td>
                    @*<td><i class="fa fa-inr"></i><span>@m.Status</span></td>*@
                    <td><i class="fa fa-inr"></i><span>@m.UserBillingAddress</span></td>
                    @*<td><i class="fa fa-inr"></i><span>@m.Amount</span></td>*@
                </tr>
            }

        </tbody>
    </table>
</div>

