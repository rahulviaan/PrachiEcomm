@model PrachiIndia.Sql.AspNetUser
@*change start here *@
<div class="modal fade" id="EditAddress">
    <div class="modal-dialog" >
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" onclick="clearvalidation();" data-dismiss="modal">
                    <span class="h2"><span class="glyphicon glyphicon-remove"></span></span>
                </button>
                <h4 class="modal-title"><span class="glyphicon glyphicon-shopping-cart"></span> <span class="small">100% Secure, Safe Online Shipping   &amp; Order Processing</span> </h4>
            </div>
            <div class="modal-body" style="background: #eee;">

                <div id="orderPageWrapper" class="orderPageWrapper" style="display: block;margin-top:-102px;">
                    <div id="orderPage" class="orderPage" style="display: block;">
                        <section id="sign-up" class="row" style="display: block;">
                            <div id="fieldProfile" class="panel-body">
                                @using (Html.BeginForm("_EditUserProfile", "UserProfile", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
                                {
                                    <div id="" class="form-group col-md-12">
                                        <h3>Complete Your Registeration</h3>
                                    </div>

                                    <div id="" class="col-md-6">
                                        <label for="txtRegisterName" class="form-control-label">Name<span>@Html.ValidationMessageFor(m => m.FirstName, "", new { @class = "text-danger" })</span></label>
                                        <div id="" class="input-group" style="width:100%">

                                            @Html.TextBoxFor(m => m.FirstName, new { @class = "form-control input-lg", id = "n" })
                                        </div>
                                    </div>
                                    <div id="" class="col-md-6">
                                        <label for="txtRegisterName" class="form-control-label">Email<span>@Html.ValidationMessageFor(m => m.Email, "", new { @class = "text-danger" })</span> </label>
                                        <div id="" class="input-group" style="width:100%">
                                            @Html.TextBoxFor(m => m.Email, new { @class = "form-control input-lg", @placeholder = "abc@gmail.com" })
                                        </div>
                                    </div>
                                    <div id="" class="col-md-6">
                                        <label for="txtRegisterPhone" class="form-control-label"> Phone <span>@Html.ValidationMessageFor(m => m.PhoneNumber, "", new { @class = "text-danger" })</span></label>
                                        <div id="" class="input-group" style="width:100%">
                                            @Html.TextBoxFor(m => m.PhoneNumber, new { @class = "form-control input-lg" })
                                        </div>
                                    </div>
                                    <div id="" class="col-md-6">
                                        <label for="txtRegisterAddress" class="form-control-label"> Address<span>@Html.ValidationMessageFor(m => m.Address, "", new { @class = "text-danger" })</span></label>
                                        <div id="" class="input-group" style="width:100%">
                                            @Html.TextBoxFor(m => m.Address, new { @class = "form-control input-lg" })
                                        </div>
                                    </div>
                                    <div class="clearfix"></div>
                                    <div id="" class="col-md-4">
                                        <label for="txtRegisterState" class="form-control-label">Country</label>
                                        <div id="" class="input-group">
                                            @Html.DropDownListFor(model => model.CountryId, ViewData["list"] as IEnumerable<SelectListItem>, "-Select Country-", new { @id = "CountryId", @class = "form-control input-lg" })
                                        </div>
                                    </div>
                                    <div id="" class="col-md-4">
                                        <label for="txtRegisterState" class="form-control-label"> State</label>
                                        <div id="" class="input-group" style="width:100%">

                                            @Html.DropDownListFor(model => model.StateId, ViewData["State"] as IEnumerable<SelectListItem>, "-Select State-", new { @id = "StateId", @class = "form-control input-lg" })
                                        </div>
                                    </div><!-- form-group -->
                                    <div id="" class="col-md-4">
                                        <label for="txtRegisterCity" class="form-control-label"> City</label>
                                        <div id="" class="input-group" style="width:100%">
                                            @Html.DropDownListFor(model => model.CityId, ViewData["City"] as IEnumerable<SelectListItem>, "-Select City-", new { @id = "CityId", @class = "form-control input-lg" })
                                        </div>
                                    </div>
                                    <div id="" class="col-md-6">
                                        <label for="txtRegisterPincode" class="form-control-label"> Pincode <span>@Html.ValidationMessageFor(m => m.PinCode, "", new { @class = "text-danger" })</span></label>
                                        <div id="" class="input-group form-control">

                                            @Html.TextBoxFor(m => m.PinCode, new { @class = "form-control input-lg" })
                                        </div>
                                        <span id="errCommon" class=""></span>
                                    </div><!-- form-group -->
                                    
                                    <div class="clearfix"></div>
                                        <div id="" class="col-md-12 text-center" style="margin-top:3%;">
                                            <div id="" class="input-group">
                                                <input type="submit" id="btnsave" class="btn btn-lg btn-primary" value="Update" />
                                            </div>
                                        </div><!-- form-group -->
                                }
                            </div><!-- col-lg-12 -->
                        </section><!-- row -->
                        <!-- ---------------------------------------------------------------- -->
                    </div><!-- orderPage -->
                </div><!-- orderPageWrapper -->
            </div>
            <div class="modal-footer">
                <div id="" class="col-lg-11 text-left text-uppercase text-muted ">
                    <p><span class="pull-left small">By creating account, building profile, signing in and sharing Personal Information, I understand and agree all the relevant terms and conditions.    We do not share any information with third party(s) in any form, without your written prior concent.</span></p>
                </div>
                <div id="" class="col-lg-1 text-center">
                    <button type="button" class="close" data-dismiss="modal">
                        <span class="h2"><span class="glyphicon glyphicon-remove"></span></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>
@*change End Here *@





