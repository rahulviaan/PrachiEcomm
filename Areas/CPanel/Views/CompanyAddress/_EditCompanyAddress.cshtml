@model PrachiIndia.Sql.tblCompanyAddress
@*change start here *@
<div class="modal fade" id="EditCompanyAddress">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content" style="border-radius: 5px;border: solid 1px #ededed; box-shadow: none;">
            <div class="modal-header">
                <button type="button" class="close" onclick="clearvalidation();" data-dismiss="modal">
                    <span class="h4"><span class="glyphicon glyphicon-remove"></span></span>
                </button>
                <div class="modal-title text-left"><span class="glyphicon glyphicon-shopping-cart"></span> <span class="small">100% Secure, Safe Online Shipping   &amp; Order Processing</span> </div>
            </div>
            <div style="background: #fafafa;">

                <div id="orderPageWrapper" class="orderPageWrapper">
                    <div id="orderPage" class="orderPage">
                        <section id="sign-up" class="panel-body">
                            <div id="fieldProfile" class="col-md-12">
                                @using (Html.BeginForm("_EditCompanyAddress", "CompanyAddress", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
                                {
                                    <div id="" class="col-md-12 text-center">
                                        <h3>Edit Here Company Address</h3>

                                    </div>
                                    <div id="editval">
                                        <div class="form-group">
                                            <div id="" class="col-md-6">
                                                <label for="txtStateName" class="form-control-label pull-left">StateName<span>@Html.ValidationMessageFor(m => m.StateName, "", new { @class = "text-danger" })</span></label>
                                                @Html.TextBoxFor(m => m.StateName, new { @class = "form-control", id = "n" })
                                            </div>
                                            <div id="" class="col-md-6">
                                                <label for="txtH_No" class="form-control-label pull-left">H_No<span>@Html.ValidationMessageFor(m => m.H_No, "", new { @class = "text-danger" })</span> </label>
                                                @Html.TextBoxFor(m => m.H_No, new { @class = "form-control", @placeholder = "abc@gmail.com" })
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div id="" class="col-md-6">
                                                <label for="txtAddress" class="form-control-label pull-left"> Address <span>@Html.ValidationMessageFor(m => m.Address, "", new { @class = "text-danger" })</span></label>
                                                @Html.TextBoxFor(m => m.Address, new { @class = "form-control" })
                                            </div>

                                            <div id="" class="col-md-6">
                                                <label for="txtNearBy" class="form-control-label pull-left"> NearBy<span>@Html.ValidationMessageFor(m => m.NearBy, "", new { @class = "text-danger" })</span></label>
                                                @Html.TextBoxFor(m => m.NearBy, new { @class = "form-control" })
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div id="" class="col-md-6">
                                                <label for="txtCity" class="form-control-label pull-left"> City <span>@Html.ValidationMessageFor(m => m.City, "", new { @class = "text-danger" })</span></label>
                                                @Html.TextBoxFor(m => m.City, new { @class = "form-control" })
                                            </div>

                                            <div id="" class="col-md-6">
                                                <label for="txtPincode" class="form-control-label pull-left"> Pincode<span>@Html.ValidationMessageFor(m => m.Pincode, "", new { @class = "text-danger" })</span></label>
                                                @Html.TextBoxFor(m => m.Pincode, new { @class = "form-control" })
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div id="" class="col-md-6">
                                                <label for="txtmobileno" class="form-control-label pull-left"> mobileno <span>@Html.ValidationMessageFor(m => m.mobileno, "", new { @class = "text-danger" })</span></label>
                                                @Html.TextBoxFor(m => m.mobileno, new { @class = "form-control" })
                                            </div>

                                            <div id="" class="col-md-6">
                                                <label for="txtEmail" class="form-control-label pull-left"> Email<span>@Html.ValidationMessageFor(m => m.Email, "", new { @class = "text-danger" })</span></label>
                                                @Html.TextBoxFor(m => m.Email, new { @class = "form-control" })
                                            </div>
                                        </div>

                                        <div class="clearfix"></div>

                                        <div class="clearfix"></div>
                                        <div class="form-group">
                                            <div id="" class="col-md-6">
                                                <label for="txtRegisterState" class="form-control-label pull-left"> State</label>
                                                @Html.DropDownListFor(model => model.Color, ViewData["classlist"] as IEnumerable<SelectListItem>, "-Select Class-", new { @id = "Colorid", @class = "form-control" })
                                            </div>
                                            <div id="" class="col-md-6">
                                                <label for="txtIsMainOffice" class="form-control-label pull-left"> IsMainOffice <span>@Html.ValidationMessageFor(m => m.IsMainOffice, "", new { @class = "text-danger" })</span></label>
                                                @Html.RadioButtonFor(m => m.IsMainOffice, new { @class = "form-control", value = "false" })
                                            </div><!-- form-group -->
                                            <span id="errCommon" class=""></span>
                                        </div>
                                    </div>
                                    <div id="" class="col-md-12 text-center">
                                        <input type="submit" id="btnsave" class="btn btn-primary" value="Save" />
                                    </div><!-- form-group -->
                                }
                            </div><!-- col-lg-12 -->
                        </section><!-- row -->
                        <!-- ------------------------------------------------------------------>
                    </div><!-- orderPage -->
                </div><!-- orderPageWrapper -->
            </div>
            <div class="modal-footer">
                <div id="" class="col-lg-12 text-center text-uppercase text-muted ">
                    <p><span class="pull-left small">Some Message For User.</span></p>
                </div>
            </div>
        </div>

    </div>
</div>
@*change End Here *@


