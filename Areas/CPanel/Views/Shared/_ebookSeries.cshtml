
@model IEnumerable<PrachiIndia.Portal.Areas.CPanel.Models.EbookSeries>
@{    string CurrnetSubject = "Init";}
@if (Model != null)
{
    foreach (var item in Model)
    {
        if (CurrnetSubject == "Init" || CurrnetSubject != item.Subject)
        {
            <tr class="danger">
                <td colspan="9" class="text-center"><b>@item.Subject</b></td>
            </tr>
            CurrnetSubject = item.Subject;
        }

        <tr id="row_@item.Id">
            <td>@item.Series <span class="bookstar" style="display:@(item.SingleStar == true ? "inline-block" : "none")"><i class="fa fa-star" aria-hidden="true"></i></span> <span class="bookstar" style="display:@(item.DoubleStar == true ? "inline-block" : "none")"><i class="fa fa-star" aria-hidden="true"></i><i class="fa fa-star" aria-hidden="true"></i></span></td>
            <td class="text-center">@item.Board</td>
            <td class="text-center"><span style="display:@(item.Audio == true ? "inline-block" : "none")" class="fa fa-check"></span><i style="color:red ;display:@(item.Audio == false ? "inline-block" : "none")" class="fa fa-times"></i></td>
            <td class="text-center"><span style="display:@(item.LessonPlan == true ? "inline-block" : "none")" class="fa fa-check"></span><i style="color:red ;display:@(item.LessonPlan == false ? "inline-block" : "none")" class="fa fa-times"></i></td>
            <td class="text-center"><span style="display:@(item.WorkSheet == true ? "inline-block" : "none")" class="fa fa-check"></span><i style="color:red ;display:@(item.WorkSheet == false ? "inline-block" : "none")" class="fa fa-times"></i></td>
            <td class="text-center"><span style="display:@(item.WorkSheetSolution == true ? "inline-block" : "none")" class="fa fa-check"></span><i style="color:red ;display:@(item.WorkSheetSolution == false ? "inline-block" : "none")" class="fa fa-times"></i></td>
            <td class="text-center"><span style="display:@(item.BookSolution == true ? "inline-block" : "none")" class="fa fa-check"></span><i style="color:red ;display:@(item.BookSolution == false ? "inline-block" : "none")" class="fa fa-times"></i></td>
            <td class="text-center"><span style="display:@(item.TestHour == true ? "inline-block" : "none")" class="fa fa-check"></span><i style="color:red ;display:@(item.TestHour == false ? "inline-block" : "none")" class="fa fa-times"></i></td>
            <td class="text-center"><span style="display:@(item.TestHourSolution == true ? "inline-block" : "none")" class="fa fa-check"></span><i style="color:red ;display:@(item.TestHourSolution == false ? "inline-block" : "none")" class="fa fa-times"></i></td>
            <td>
                <button class="btn btn-success" data-toggle="modal" data-target="#myModal1" onclick="AddEditebookSeries(@item.Id)"><i class="glyphicon glyphicon-pencil"></i></button>
                <button class="btn btn-danger" data-toggle="modal" data-target="#myModal" onclick="ConfirmDelete(@item.Id)"><i class="glyphicon glyphicon-trash"></i></button>
                </td>

        </tr>
    }

}
else
{
    <tr>
        <td colspan="10" style="color:red;font:bold;text-align:center"><h3> No Data Found</h3> </td>

    </tr>
}