
@{
    ViewBag.Title = "WorkShop";
}
@section metatags{
    <title>Prachi Group - Educational Books Publishers India</title>
    <meta name="keywords" content="books, epublishing, digital publishing, digital conversion, digital transformation, ebook publishing, ebook conversion, ebook transformation, ebook creation, content publishing, content conversion, content transformation, content creation, content enrichment, ebook enhancement, ebook consulting, epub publishing, epub conversion, epub transformation, xml consulting, xml publishing, xml conversion, xml transformation">
    <meta name="description" content="We are a leading services company providing content solutions, data analytics and related services to publishers, information providers, and enterprises.">
}
<section class="block-parallax-1 headdingimg">
    <div class="parallax-bg"></div>
    <div class="md-box">
        <div class="container">
            <div class="panel-body" style="padding-bottom: 0;padding-top:0;">
                <div class="col-md-12 headtitle text-center">
                    <h2>Work Shop</h2>
                </div>
                <div class="col-md-12 text-center">
                    <ul class="innerbradcrum">
                        <li><a href="@Url.Action("Index","Home")">Home</a></li>
                        <li>Work Shop</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</section>
<div class="clearfix"></div>
<section id="about" style="padding:10px;">
    <div class="container col-md-10 col-md-offset-1">
        <div class="panel-body">
            <div class="panel panel-default">
                <div class="panel-heading" style="background-color:#f65121;color: #ffffff;">
                    <div class="panel-title text-center">Class and Stress Management Seminar at Ghaziabad</div>
                </div>
                <div class="panel-body" style="padding-top:0">
                    <div class="col-md-12 text-right">
                        <p style="font-size:12px;margin: 4px 0;"><i class="fa fa-calendar" aria-hidden="true"></i> July 31, 2016 : 04:39 PM to 02:15 AM</p>
                    </div>
                    <div class="col-xs-6 col-sm-6 col-md-6">
                        <p style="width:100%; float:left;"><span class="organiser"><b>Delivered By</b></span></p>
                        <p><i class="fa fa-user" aria-hidden="true"></i> Neeraj Kumar Singh</p>
                    </div>
                    <div class="col-xs-6 col-sm-6 col-md-6">
                        <p style="width:100%; float:left;"><span class="organiser"><b>Location</b></span></p>
                        <div class="media">
                            <div class="media-left">
                                <i class="fa fa-map-marker" aria-hidden="true"></i>
                            </div>
                            <div class="media-body">
                                <p style="padding-left:2%;">309/10, ALLIED HOUSE, INDER LOK, DELHI - 110035</p>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"><hr></div>
                    <div class="panel-body">
                        <div id="carousel-example" class="carousel slide" data-ride="carousel">
                            <ol class="carousel-indicators">
                                <li data-target="#carousel-example" data-slide-to="0" class="active"></li>
                                <li data-target="#carousel-example" data-slide-to="1"></li>
                                <li data-target="#carousel-example" data-slide-to="2"></li>
                            </ol>

                            <div class="carousel-inner">
                                <div class="item active">
                                    <a href="#"><img src="/img/teacher-assistance/TA-1.1.png" /></a>
                                    </div>
                                <div class="item">
                                    <a href="#"><img src="/img/teacher-assistance/TA-1.2.png" /></a>
                                    </div>
                                <div class="item">
                                    <a href="#"><img src="/img/teacher-assistance/amritsar-img2.jpg" /></a>
                                    </div>
                            </div>

                            <a class="left carousel-control" href="#carousel-example" data-slide="prev">
                                <span class="glyphicon glyphicon-chevron-left"></span>
                            </a>
                            <a class="right carousel-control" href="#carousel-example" data-slide="next">
                                <span class="glyphicon glyphicon-chevron-right"></span>
                            </a>
                        </div>
                       </div>
                    <div class="clearfix"></div>
                    <div class="col-xs-12 col-sm-12 col-md-12">
                        <div class="panel-body" style="background-color:#e8f6ff;">
                            <p style="width:100%; float:left;"><span class="organiser"><b>Description</b></span></p>
                            <p class="pull-left">The seminar was held at Mother India School in Ghaziabad. It was aimed at empowering the teachers to overcome the crucial challenges in terms of class management.</p>
                        </div>
                    </div>
                    <div class="clearfix"><hr></div>
                    <div class="col-xs-12 col-sm-12 col-md-12">
                        <div style="background-color:#f0f9e8;float: left;width: 100%;" class="panel-body">
                            <p style="width:100%; float:left;"><span class="organiser"><b>Organisers</b></span></p>
                            <p class="pull-left">Satish Chandra<br>Prachi India Pvt Ltd<br>Bal Bharti School New Delhi<br>+91 - 42285451</p>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
    </div>
</section>
<div class="clearfix"></div>
@section scripts{
    <style>
       
        .organiser {
            border-bottom: solid 1px rgba(4, 21, 207, 0.1);
            display: inline;
            float: left;
            padding: 3px 0;
            margin-bottom: 0;
            font-size: 13px;
            text-transform: uppercase;
        }

        .fit-images {
            width: 100%;
            height: 150px;
            object-fit: cover;
            margin-top: 2%;
        }

        .media {
            background-color: #ecfef9;
            padding-left: 10px;
        }

        .media-body, .media-left, .media-right {
            display: table-cell;
            vertical-align: top;
        }

        .workshopdiv li a {
            color: #000000 !important;
            font-size: 15px;
            padding: 15px 20px !important;
        }
         .carousel-inner > .item > a > img {
    display: block;
    height: 320px;
    max-width: 100%;
    line-height: 1;
    max-height: 100%;
    width: 100%;
    object-fit: cover;
}
        .carousel-control.right {
            right: 3.5%!important;
        }
    </style>
}

