                            <div class="panel-body">
                                @if (Model != null && Model.Any())
                                {
                                    foreach (var item in Model)
                                    {
                                        <div class="row">
                                            @if (string.IsNullOrWhiteSpace(item.Image))
                                            {
                                                <div class="col-md-12">
                                                    <h4>
                                                        <a href="@Url.Action("SeriesDetail","Home",new {id=item.Id })">
