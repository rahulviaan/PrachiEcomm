@section scripts{
    @*<link href="~/Content/js/form/perfect-scrollbar.css" rel="stylesheet" />
        <script src="~/Content/js/form/perfect-scrollbar.js"></script>*@
    <style>
        .panel-heading a {
            float: none;
            background-color: transparent;
            border-bottom: transparent;
        }

        .nav > li > a {
            padding: 10px 4px !important;
            font-size: 14px;
            outline: none;
        }

        .nav.nav-tabs a {
            color: #50599e;
        }

        .nav-tabs > li.active > a, .nav-tabs > li.active > a:hover, .nav-tabs > li.active > a:focus {
            background-color: #e9fafe;
            border: transparent;
            border-bottom-color: transparent;
            border-bottom-color: transparent;
        }

        .sub-tab li {
            width: 100%;
            float: left;
            border-bottom: solid 1px #f7f7f7;
        }

        .nav.nav-tabs a:hover {
            background-color: #e4fbfe;
            border-radius: 0;
            border: solid 1px #e4fbfe;
        }

        .panel-heading a:hover {
            text-decoration: none;
        }

        .nav-tabs {
            border-bottom: none;
        }
       .panel-default > .panel-heading {
            background-color: #ffffff!important;
            border-color: #f2f2f2!important;
        }
       .panel-default {
    border-color: #f6f1f1!important;
}
    </style>
    <script>
        var $ = document.querySelector.bind(document);

        new PerfectScrollbar('#leftmenusub');
        setTimeout(() => $('#leftmenusub').classList.add('ps--focus'), 0);
        setTimeout(() => $('#leftmenusub').classList.remove('ps--focus'), 750);
    </script>
}
