@model PrachiIndia.Sql.AspNetUser
@*change start here *@
<div class="modal fade" id="EditAddress">
    <div class="modal-dialog modal-lg">
        <!-- Modal content-->
        <div class="modal-content col-md-8 col-md-offset-2" style="border-radius: 5px;border: solid 1px #ededed; box-shadow: none;">

            <div class="modal-header">
                <button type="button" class="close" onclick="clearvalidation();" data-dismiss="modal" style="margin-top: -21px;">
                    <span class="h4"><span class="glyphicon glyphicon-remove"></span></span>
                </button>
                <h4 class="modal-title text-center">Update Your Profile </h4>
            </div>
            
            <div class="modal-body" style="background: #ffffff;">

                <div id="orderPageWrapper" class="orderPageWrapper">
                    <div id="orderPage" class="orderPage">
                        <section id="sign-up" class="panel-body">
                            <div id="fieldProfile" class="col-md-12">
                                @using (Html.BeginForm("_EditUserProfile", "Account", FormMethod.Post, new { @class = "form-horizontal", role = "form", enctype = "multipart/form-data" }))
                                {

                                    <div class="form-group">
                                        <div id="" class="col-md-6">
                                            <label for="txtRegisterName" class="form-control-label pull-left">Name<span>@Html.ValidationMessageFor(m => m.FirstName, "", new { @class = "text-danger" })</span></label>
                                            @Html.TextBoxFor(m => m.FirstName, new { @class = "form-control updateprf", id = "n" })
                                        </div>
                                        <div id="" class="col-md-6">
                                            <label for="txtRegisterName" class="form-control-label pull-left">Email<span>@Html.ValidationMessageFor(m => m.Email, "", new { @class = "text-danger" })</span> </label>
                                            @Html.TextBoxFor(m => m.Email, new { @class = "form-control updateprf", @placeholder = "abc@gmail.com" })
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div id="" class="col-md-6">
                                            <label for="txtRegisterPhone" class="form-control-label pull-left"> Phone <span>@Html.ValidationMessageFor(m => m.PhoneNumber, "", new { @class = "text-danger" })</span></label>
                                            @Html.TextBoxFor(m => m.PhoneNumber, new { @class = "form-control updateprf" })
                                        </div>

                                        <div id="" class="col-md-6">
                                            <label for="txtRegisterAddress" class="form-control-label pull-left"> Address<span>@Html.ValidationMessageFor(m => m.Address, "", new { @class = "text-danger" })</span></label>
                                            @Html.TextBoxFor(m => m.Address, new { @class = "form-control updateprf" })
                                        </div>
                                    </div>
                                    <div class="clearfix"></div>
                                    <div class="form-group">
                                        <div id="" class="col-md-4">
                                            <label for="txtRegisterState" class="form-control-label pull-left">Country</label>
                                            @Html.DropDownListFor(model => model.CountryId, ViewData["list"] as IEnumerable<SelectListItem>, "-Select Country-", new { @id = "CountryId", @class = "form-control updateprf" })
                                        </div>
                                        <div id="" class="col-md-4">
                                            <label for="txtRegisterState" class="form-control-label pull-left"> State</label>
                                            @Html.DropDownListFor(model => model.StateId, ViewData["State"] as IEnumerable<SelectListItem>, "-Select State-", new { @id = "StateId", @class = "form-control updateprf" })
                                        </div><!-- form-group -->
                                        <div id="" class="col-md-4">
                                            <label for="txtRegisterCity" class="form-control-label pull-left"> City</label>
                                            @Html.DropDownListFor(model => model.CityId, ViewData["City"] as IEnumerable<SelectListItem>, "-Select City-", new { @id = "CityId", @class = "form-control updateprf" })
                                        </div>
                                    </div>
                                    <div class="clearfix"></div>
                                    <div class="form-group">
                                        <div id="" class="col-md-6">
                                            <label for="txtRegisterPincode" class="form-control-label pull-left"> Pincode <span>@Html.ValidationMessageFor(m => m.PinCode, "", new { @class = "text-danger" })</span></label>
                                            @Html.TextBoxFor(m => m.PinCode, new { @class = "form-control updateprf" })
                                        </div><!-- form-group -->
                                        <div id="" class="col-md-6">
                                            <label for="txtRegisterPincode" class="form-control-label pull-left">Upload Image</label>
                                            <input name="UserImage" type="file" class="form-control updateprf" />
                                            <span id="errCommon" class=""></span>
                                        </div>
                                    </div>
                                    <div class="col-md-12"><br /><hr /><br /></div>
                                    <div id="" class="col-md-12 text-center">
                                        <input type="submit" id="btnsave" class="btn btn-primary signindiv" value="Update" />
                                    </div><!-- form-group -->
                                }
                            </div><!-- col-lg-12 -->
                        </section><!-- row -->
                        <!-- ------------------------------------------------------------------>
                    </div><!-- orderPage -->
                </div><!-- orderPageWrapper -->
            </div>

        </div>

    </div>
</div>
@*change End Here *@





